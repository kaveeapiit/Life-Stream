  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "license": "ISC",
  "author": "",
  "type": "module",
  "main": "eslint.config.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/index.js",
    "client": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:backend": "cd backend && npm test",
    "test:selenium": "node tests/selenium/ComprehensiveTests.js",
    "test:selenium:simplified": "node tests/selenium/runSimplified.js",
    "test:selenium:proper": "node tests/selenium/runProperTests.js",
    "test:selenium:database": "node tests/selenium/runDatabaseValidation.js",
    "test:selenium:simple": "node tests/selenium/SimpleTests.js",
    "test:selenium:login": "node tests/selenium/LoginTests.js",
    "test:all": "npm run test && npm run test:backend && npm run test:e2e"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
